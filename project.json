{
  "projectId": "prj_kL1emTw7mFQjuwE9REIXPfzPRrMf",
  "orgId": "team_nfVlELaBqrCmu615QTvL4AfS",
  "projectName": "api-red-social",

  "functions": {
    "main_api/index.js": {
      "runtime": "nodejs18.x"
    },
    "post/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/auth/:path*",
      "destination": "/main_api/index.js"
    },
    {
      "source": "/api/comments/:path*",
      "destination": "/main_api/index.js"
    },
    {
      "source": "/api/user/:path*",
      "destination": "/main_api/index.js"
    },
    {
      "source": "/api/post/:path*",
      "destination": "/post/index.js"
    },
    {
      "source": "/api/mysql/:path*",
      "destination": "/mysql/index.js"
    }
  ]
}
